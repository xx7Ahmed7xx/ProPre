<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <meta name="author" content="prepro team" />
    <meta name="description" content="genomics project" />
    <title>Protien Prediction</title>
    <!-- bootstrap -->
    <link
      href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css"
      rel="stylesheet"
      integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3"
      crossorigin="anonymous"
    />
    <!-- main css -->
    <link rel="stylesheet" href="~/css/style.css" />
    @await RenderSectionAsync("Styles", required: false)
</head>
<body>
    <div class="app container">
      <section class="header">
        <div class="header-top">
          <h3 class="logo"><span style="color: #fee100">Pre</span>Pro</h3>
          <div class="search">
            <form>
              <input type="text" />
              <input type="submit" value="" />
            </form>
          </div>
        </div>
        <nav class="navbar navbar-expand-lg navbar-dark bg-dark px-2">
          <div class="container-fluid">
            <button
              class="navbar-toggler"
              type="button"
              data-bs-toggle="collapse"
              data-bs-target="#navbarSupportedContent"
              aria-controls="navbarSupportedContent"
              aria-expanded="false"
              aria-label="Toggle navigation"
            >
              <span class="navbar-toggler-icon"></span>
            </button>
            <div class="collapse navbar-collapse" id="navbarSupportedContent">
              <ul class="navbar-nav me-auto mb-2 mb-lg-0">
                <li class="nav-item">
                  <a
                    class="nav-link @ViewData["IndexActive"]"
                    aria-current="page"
                    href="/Home/Index"
                    >Home</a
                  >
                </li>
                <li class="nav-item">
                  <a class="nav-link @ViewData["ProtienActive"]" href="/Home/protien">Protien</a>
                </li>
                <li class="nav-item">
                  <a class="nav-link @ViewData["DnaActive"]" href="/Home/dna">DNA</a>
                </li>
              </ul>
            </div>
          </div>
        </nav>
      </section>
    <section class="main-wrapper mt-3">
        @RenderBody()
      </section>
    <section class="footer bg-dark p-3">
        <h3>
          All rights reserved &copy; <span style="color: #fee100">Pro</span>pre
          team 2022
        </h3>
      </section>
      </div>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
